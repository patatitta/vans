---

---


<div 
class="overflow-hidden"
  x-data="{
    expandedNav: false,
    isMobile: false,
    mostrarNuevo: true,
    mostrarOtros: false,
    mostrarMujer: false,
    mostrarNovedades: false,
    mostrarGeneral: true,
    mostrarTendencias: false,
    mostrarDestacados: false,
    mostrarClassics: false,
    mostrarActionSports: false,
    mostrarZapatillas: false,
    mostrarRopa: false,
    mostrarAccesorios: false,
    mostrarHombre: false,
    mostrarKids: false,
    init() {
      this.isMobile = window.innerWidth < 768;
      window.addEventListener('resize', () => {
        this.isMobile = window.innerWidth < 768;
      });
    }
  }"
  x-init="init()"
  @toggle-nav1.window="expandedNav = !expandedNav"
  @toggle-nav2.window="expandedNav = !expandedNav"
>
  <!-- Menu -->
  <div x-cloak
    class="fixed top-0 left-0 w-full md:w-100 h-screen bg-black/70 backdrop-blur-sm z-50 text-neutral-300 transform transition duration-300 ease-in-out overflow-hidden"
    :class="{'translate-x-0 translate-y-0': expandedNav, '-translate-x-full': !expandedNav && !isMobile, 'translate-y-full': !expandedNav && isMobile}" > 


<div
    id="cerrar" 
      @click="expandedNav = !expandedNav"

    class="absolute bottom-7 right-7 inline-flex items-center justify-center cursor-pointer w-12 h-12 bg-white z-50"
  >
    <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 32 32" id="close" class="w-8 h-8">
      <path d="m25.54 8-8 8 8 8L24 25.54l-8-8-8 8L6.46 24l8-8-8-8L8 6.46l8 8 8-8L25.54 8Z"></path>
    </svg>
  </div>



  <div class="flex flex-col h-full" id="principal" x-show="mostrarGeneral">
    <div>
      <ul class="p-6 space-y-4 flex" id="categorias" >
        <li>
          <div
            class="p-1 border-2 border-transparent hover:border-white transition-all duration-200 ease-in-out cursor-pointer"
            id="newLabel"
  @click="mostrarNuevo = true, mostrarMujer = false, mostrarHombre=false, mostrarKids=false;"
  :class="{'bg-white': mostrarNuevo, 'text-black': mostrarNuevo,}"
            
          >
            Nuevo y destacado
          </div>
        </li>
        <li>
          <div
            class="p-1 border-2 border-transparent hover:border-white transition-all duration-200 ease-in-out cursor-pointer"
            id="mujerLabel"
  @click="mostrarMujer = !mostrarMujer, mostrarNuevo= false, mostrarHombre = false, mostrarKids=false"
  :class="{'bg-white': mostrarMujer, 'text-black': mostrarMujer, }" >
            Mujer
          </div>
        </li>
        <li>
          <div
            class="p-1 border-2 border-transparent hover:border-white transition-all duration-200 ease-in-out cursor-pointer"
            id="hombreLabel"
              @click="mostrarHombre = true, mostrarNuevo= false, mostrarMujer = false, mostrarKids=false;"
  :class="{'bg-white': mostrarHombre, 'text-black': mostrarHombre, }" 
          >
            Hombres
          </div>
        </li>
        <li>
          <div
            class="p-1 border-2 border-transparent hover:border-white transition-all duration-200 ease-in-out cursor-pointer"
            id="kidsLabel"
              @click="mostrarKids = true, mostrarNuevo= false, mostrarMujer = false, mostrarHombre = false;"
          :class="{'bg-white': mostrarKids, 'text-black': mostrarKids, }" 

          >
            Niños
          </div>
        </li>
      </ul>
        <div class="relative min-h-80 overflow-hidden">

      <!-- NUEVO Y DESTACADO -->
<div x-show="mostrarNuevo" class="absolute top-0 left-0 w-full"
     x-transition:enter="transition-transform duration-200"
     x-transition:enter-start="translate-x-full opacity-0"
     x-transition:enter-end="translate-x-0 opacity-100"
     x-transition:leave="transition-transform duration-200"
     x-transition:leave-start="translate-x-0 opacity-100"
     x-transition:leave-end="translate-x-full opacity-0">

        <ul 
          class="space-y-2 *:transition-all duration-150 ml-7 *:mb-4 *:mr-4"
          id="new"
        >
           <li class="flex mb-0 justify-between">
            <div class="relative inline-block group hover:-rotate-2">
              <span class="relative cursor-pointer" id="valentinoGaraavni"
                >Valentino Garaavni y Vans</span
              >
              <span
                class="absolute bottom-0 left-0 h-0.5 bg-white w-0 group-hover:w-full transition-all duration-500"
              ></span>
            </div>
          </li>

           <li class="flex mb-0 justify-between">
            <div class="relative inline-block group hover:-rotate-2">
              <span class="relative cursor-pointer" id="kPop"
                >Kpop demon hunters</span
              >
              <span
                class="absolute bottom-0 left-0 h-0.5 bg-white w-0 group-hover:w-full transition-all duration-500"
              ></span>
            </div>
          </li>


          
          <li class="flex mb-0 justify-between">
            <div class="relative inline-block group hover:-rotate-2">
              <span class="relative cursor-pointer" id="novedades"   @click="mostrarNovedades = !mostrarNovedades, mostrarGeneral=false"

                >Novedades</span
              >
              <span
                class="absolute bottom-0 left-0 h-0.5 bg-white w-0 group-hover:w-full transition-all duration-500"
              ></span>
            </div>
            <svg
              class="w-6 h-6"
              xmlns="http://www.w3.org/2000/svg"
              viewBox="0 0 32 32"
            >
              <path
                fill="currentColor"
                d="M9.91 24.96 19.37 16 9.9 7.04l1.5-1.58L22.54 16 11.41 26.54l-1.5-1.58Z"
              ></path>
            </svg>
          </li>

          <li class="flex mb-0 justify-between">
            <div class="relative inline-block group hover:-rotate-2">
              <span class="relative cursor-pointer" id="tendencia" @click="mostrarTendencias = !mostrarTendencias, mostrarGeneral=false"
                >Tendencia ahora</span
              >
              <span
                class="absolute bottom-0 left-0 h-0.5 bg-white w-0 group-hover:w-full transition-all duration-500"
              ></span>
            </div>
            <svg
              class="w-6 h-6"
              xmlns="http://www.w3.org/2000/svg"
              viewBox="0 0 32 32"
            >
              <path
                fill="currentColor"
                d="M9.91 24.96 19.37 16 9.9 7.04l1.5-1.58L22.54 16 11.41 26.54l-1.5-1.58Z"
              ></path>
            </svg>
          </li>

          <li class="flex mb-0 justify-between">
            <div class="relative inline-block group hover:-rotate-2">
              <span class="relative cursor-pointer" id="destacados" @click="mostrarDestacados = !mostrarDestacados, mostrarGeneral=false"
                >Destacados</span
              >
              <span
                class="absolute bottom-0 left-0 h-0.5 bg-white w-0 group-hover:w-full transition-all duration-500"
              ></span>
            </div>
            <svg
              class="w-6 h-6"
              xmlns="http://www.w3.org/2000/svg"
              viewBox="0 0 32 32"
            >
              <path
                fill="currentColor"
                d="M9.91 24.96 19.37 16 9.9 7.04l1.5-1.58L22.54 16 11.41 26.54l-1.5-1.58Z"
              ></path>
            </svg>
          </li>




          <li class="flex mb-0 justify-between">
            <div class="relative inline-block group hover:-rotate-2">
              <span class="relative cursor-pointer" id="classics" @click="mostrarClassics = !mostrarClassics, mostrarGeneral=false"
                >Classics</span
              >
              <span
                class="absolute bottom-0 left-0 h-0.5 bg-white w-0 group-hover:w-full transition-all duration-500"
              ></span>
            </div>
            <svg
              class="w-6 h-6"
              xmlns="http://www.w3.org/2000/svg"
              viewBox="0 0 32 32"
            >
              <path
                fill="currentColor"
                d="M9.91 24.96 19.37 16 9.9 7.04l1.5-1.58L22.54 16 11.41 26.54l-1.5-1.58Z"
              ></path>
            </svg>
          </li>





          <li class="flex mb-0 justify-between">
            <div class="relative inline-block group hover:-rotate-2">
              <span class="relative cursor-pointer" id="actionSports" @click="mostrarActionSports = !mostrarActionSports, mostrarGeneral=false"
                >Action Sports</span
              >
              <span
                class="absolute bottom-0 left-0 h-0.5 bg-white w-0 group-hover:w-full transition-all duration-500"
              ></span>
            </div>
            <svg
              class="w-6 h-6"
              xmlns="http://www.w3.org/2000/svg"
              viewBox="0 0 32 32"
            >
              <path
                fill="currentColor"
                d="M9.91 24.96 19.37 16 9.9 7.04l1.5-1.58L22.54 16 11.41 26.54l-1.5-1.58Z"
              ></path>
            </svg>
          </li>


          <li class="relative inline-block group">
            <span class="relative cursor-pointer">Rebajas</span>
            <span
              class="absolute bottom-0 left-0 h-0.5 bg-white w-0 group-hover:w-full transition-all duration-500"
            ></span>
          </li>
        </ul>
      </div>

      <!-- MUJER -->
<div x-show="mostrarMujer || mostrarHombre || mostrarKids"  class="absolute top-0 left-0 w-full"
     x-transition:enter="transition-transform duration-200"
     x-transition:enter-start="translate-x-full opacity-0"
     x-transition:enter-end="translate-x-0 opacity-100"
     x-transition:leave="transition-transform duration-200"
     x-transition:leave-start="translate-x-0 opacity-100"
     x-transition:leave-end="translate-x-full opacity-0">

        <ul
          class="space-y-2  *:transition-all duration-150 ml-7 *:mb-4 *:mr-4"
          id="woman"
        >
           <li class="flex mb-0 justify-between">
            <div class="relative inline-block group hover:-rotate-2">
              <span class="relative cursor-pointer" id="verTodoOtro"
                >Ver todo</span
              >
              <span
                class="absolute bottom-0 left-0 h-0.5 bg-white w-0 group-hover:w-full transition-all duration-500"
              ></span>
            </div>
          </li>

           <li class="flex mb-0 justify-between">
            <div class="relative inline-block group hover:-rotate-2">
              <span class="relative cursor-pointer" id="valentinoGaraavniOtros"
                >Valentino Garaavni y Vans</span
              >
              <span
                class="absolute bottom-0 left-0 h-0.5 bg-white w-0 group-hover:w-full transition-all duration-500"
              ></span>
            </div>
          </li>
          
           <li class="flex mb-0 justify-between">
            <div class="relative inline-block group hover:-rotate-2">
              <span class="relative cursor-pointer" id="kpopOtros"
                >Kpop demon hunters</span
              >
              <span
                class="absolute bottom-0 left-0 h-0.5 bg-white w-0 group-hover:w-full transition-all duration-500"
              ></span>
            </div>
          </li>

           <li class="flex mb-0 justify-between">
            <div class="relative inline-block group hover:-rotate-2">
              <span class="relative cursor-pointer" id="nuevoDestacado"
                >Nuevo y Destacado</span
              >
              <span
                class="absolute bottom-0 left-0 h-0.5 bg-white w-0 group-hover:w-full transition-all duration-500"
              ></span>
            </div>
          </li>


          <li class="flex mb-0 justify-between">
            <div class="relative inline-block group hover:-rotate-2">
              <span class="relative cursor-pointer" id="zapatillas" @click="mostrarZapatillas = !mostrarZapatillas, mostrarGeneral=false"
                >Zapatillas</span
              >
              <span
                class="absolute bottom-0 left-0 h-0.5 bg-white w-0 group-hover:w-full transition-all duration-500"
              ></span>
            </div>
            <svg
              class="w-6 h-6"
              xmlns="http://www.w3.org/2000/svg"
              viewBox="0 0 32 32"
            >
              <path
                fill="currentColor"
                d="M9.91 24.96 19.37 16 9.9 7.04l1.5-1.58L22.54 16 11.41 26.54l-1.5-1.58Z"
              ></path>
            </svg>
          </li>





          <li class="flex mb-0 justify-between">
            <div class="relative inline-block group hover:-rotate-2">
              <span class="relative cursor-pointer" id="ropa" @click="mostrarRopa = !mostrarRopa, mostrarGeneral=false"
                >Ropa</span
              >
              <span
                class="absolute bottom-0 left-0 h-0.5 bg-white w-0 group-hover:w-full transition-all duration-500"
              ></span>
            </div>
            <svg
              class="w-6 h-6"
              xmlns="http://www.w3.org/2000/svg"
              viewBox="0 0 32 32"
            >
              <path
                fill="currentColor"
                d="M9.91 24.96 19.37 16 9.9 7.04l1.5-1.58L22.54 16 11.41 26.54l-1.5-1.58Z"
              ></path>
            </svg>
          </li>

           <li class="flex mb-0 justify-between">
            <div class="relative inline-block group hover:-rotate-2">
              <span class="relative cursor-pointer" id="accesorios" @click="mostrarAccesorios = !mostrarAccesorios, mostrarGeneral=false"
                >Accesorios</span
              >
              <span
                class="absolute bottom-0 left-0 h-0.5 bg-white w-0 group-hover:w-full transition-all duration-500"
              ></span>
            </div>
            <svg
              class="w-6 h-6"
              xmlns="http://www.w3.org/2000/svg"
              viewBox="0 0 32 32"
            >
              <path
                fill="currentColor"
                d="M9.91 24.96 19.37 16 9.9 7.04l1.5-1.58L22.54 16 11.41 26.54l-1.5-1.58Z"
              ></path>
            </svg>
          </li>


          <li class="relative inline-block group hover:-rotate-2">
            <span class="relative cursor-pointer">Rebajas</span>
            <span
              class="absolute bottom-0 left-0 h-0.5 bg-white w-0 group-hover:w-full transition-all duration-500"
            ></span>
          </li>
        </ul>
      </div>
    </div>
</div>
    <!-- LOGOS -->
    <div class="flex grow items-center" x-show="mostrarGeneral">
      <ul
        class="grow border-t-white border-b-white border-solid border py-6 *:ml-8 border-l-0 border-r-0"
      >
        <li>
          <svg xmlns="http://www.w3.org/2000/svg" width="67" height="24">
